{
  "expo": {
    "name": "Celo React Native Truffle",
    "slug": "celo-rn-truffle",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/white-wallet-rings.png",
    "scheme": "celodappkit",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Un repositorio con la estructura general de un proyecto del blockchain Celo en react native utilizando truffle.",
    "githubUrl": "https://github.com/nestorbonilla/celo-rn-truffle"
  }
}
